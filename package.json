{
  "private": true,
  "name": "vue-starter-template",
  "description": "Vue Starter Template is an opinionated Vue 3/TypeScript/Naive UI/Tailwind CSS starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type:check && pnpm build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "cspell:check": "npx cspell \"**\"",
    "cspell:sort": "node ./scripts/cspell-sort.js",
    "eslint:check": "eslint \"**/*.{ts,tsx,js,tsx,vue}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --check --write --ignore-unknown .",
    "type:check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "nprogress": "0.2.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@brucesong/eslint-config-vue": "^1.0.6",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.103",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.5.0",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "autoprefixer": "^10.4.15",
    "cspell": "^7.0.0",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "naive-ui": "^2.34.4",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "sass": "^1.66.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vfonts": "^0.0.3",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.2.0"
  },
  "license": "MIT"
}
