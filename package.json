{
  "name": "vue-ts-starter-template",
  "description": "üçÄ An opinionated Vue 3/TypeScript starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type:check && pnpm build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "cspell:check": "npx cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx,js,tsx,vue}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "type:check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "cz": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "nprogress": "0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@brucesong/eslint-config-vue": "^1.0.15",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@iconify/json": "^2.2.128",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jsdom": "^21.1.3",
    "@types/nprogress": "^0.2.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cspell": "^7.3.8",
    "cz-git": "^1.7.1",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "naive-ui": "^2.35.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "vfonts": "^0.0.3",
    "vite": "^4.4.11",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vue-tsc": "^1.8.19"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "private": true,
  "license": "MIT"
}
